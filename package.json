{
    "name": "developer-platform-vscode-chat",
    "publisher": "ms-vscode",
    "displayName": "Developer Platform Chat Agent",
    "description": "Microsoft Developer Platform chat agent for VS Code",
    "repository": {
        "url": "https://github.com/Microsoft/developer-platform-vscode-chat"
    },
    "version": "99.99.99",
    "engines": {
        "vscode": "^1.83.0-20230923"
    },
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "enabledApiProposals": [
        "chatAgents2",
        "chatRequestAccess",
        "interactive",
        "readonlyMessage"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "generate-devplat-api-types": "npx openapi-typescript https://devplatformtestapi.azurewebsites.net/openapi/v1/openapi.yaml -o src/domain/devplat-api-openapi.ts"
    },
    "devDependencies": {
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.19",
        "@types/lunr": "^2.3.5",
        "@types/node": "^20.5.9",
        "@types/vscode": "1.82.0",
        "@types/json-schema": "^7.0.15",
        "eslint": "^7.22.0",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3"
    },
    "dependencies": {
        "dotenv": "^16.3.1",
        "lunr": "^2.3.9"
    }
}